{
    "name": "chessy-backend",
    "private": true,
    "version": "0.0.0",
    "proxy": "http://localhost:8080",
    "scripts": {
      "dev": "vite",
      "build": "vite build",
      "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
