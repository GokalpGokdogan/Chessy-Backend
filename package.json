      "jsonwebtoken": "^9.0.2",
      "jwt-decode": "^4.0.0",
      "lodash-es": "^4.17.21",
      "mongodb": "^6.3.0",
      "mongoose": "^8.0.4",
      "stockfish": "github:nmrugg/stockfish.js",
      "validator": "^13.11.0"
    },
    "devDependencies": {
      "@types/react": "^18.2.43",
      "@types/react-dom": "^18.2.17",
      "@vitejs/plugin-react": "^4.2.1",
      "autoprefixer": "^10.4.16",
      "concurrently": "^8.2.2",
      "eslint": "^8.55.0",
      "eslint-plugin-react": "^7.33.2",
      "eslint-plugin-react-hooks": "^4.6.0",
      "eslint-plugin-react-refresh": "^0.4.5",
